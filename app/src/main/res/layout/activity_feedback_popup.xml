<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="619dp"
    tools:context=".RulesPopup"
    android:background="#faf1b4">

    <TextView
        android:id="@+id/Rules"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:gravity="center"
        android:text="FEEDBACK"
        android:textColor="@color/black"
        android:textSize="36sp">

    </TextView>

    <TextView
        android:id="@+id/Rules1"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_below="@+id/Rules"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="IF YOU HAVE ANY QUESTIONS, FEEDBACK, OR WANT TO REPORT A BUG"
        android:textColor="@color/black"
        android:textSize="20sp">

    </TextView>

    <TextView
        android:id="@+id/Rules2"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_below="@+id/Rules1"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="PLEASE DO NOT HESITATE TO CONTACT US"
        android:textColor="@color/black"
        android:textSize="20sp">

    </TextView>

    <TextView
        android:id="@+id/Rules3"
        android:layout_width="match_parent"
        android:layout_height="99dp"
        android:layout_below="@+id/Rules2"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="GROUPSIXNINEMENMORRIS@GMAIL.COM"
        android:textColor="@color/black"
        android:textSize="20sp">

    </TextView>


</RelativeLayout>
